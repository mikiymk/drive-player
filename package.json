{
  "name": "drive-player",
  "version": "0.7.1",
  "description": "music files in google drive play on browser",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update-snapshot": "vitest run --update",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "start": "vite",
    "check": "run-z format type lint lint2 test",
    "format": "prettier -w index.html src api",
    "lint": "eslint src api",
    "lint:fix": "eslint src api --fix",
    "lint2": "biome check .",
    "lint2:fix": "biome check . --write",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "watch": "run-z type:watch start -p",
    "vercel:dev": "vercel dev",
    "vercel:prev": "vercel",
    "vercel:prod": "vercel --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/drive-player.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/drive-player/issues"
  },
  "homepage": "https://github.com/mikiymk/drive-player#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.17.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vanilla-extract/vite-plugin": "^3.9.5",
    "@vercel/node": "^3.2.29",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-solid": "~0.13.2",
    "jsdom": "^24.1.3",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "run-z": "^2.1.0",
    "typescript": "^5.7.3",
    "vercel": "^33.7.1",
    "vite": "^5.4.14",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.17.5",
    "vite-plugin-solid": "^2.11.1",
    "vitest": "^1.6.1",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.17.1",
    "buffer": "^6.0.3",
    "destyle.css": "^4.0.1",
    "events": "^3.3.0",
    "music-metadata": "^8.3.0",
    "process": "^0.11.10",
    "solid-js": "^1.9.4"
  }
}
