{
  "name": "drive-player",
  "version": "0.7.0",
  "description": "music files in google drive play on browser",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "run-s build:js",
    "build:js": "vite build",
    "build:analyze": "vite build --mode analyze",
    "start": "vite",
    "check": "run-s format lint type test",
    "format": "prettier -w index.html src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "watch": "run-p type:watch start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/drive-player.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/drive-player/issues"
  },
  "homepage": "https://github.com/mikiymk/drive-player#readme",
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.1",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vanilla-extract/vite-plugin": "^3.1.6",
    "eslint": "^8.15.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-solid": "~0.4.7",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.0",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-solid": "^2.2.6",
    "vitest": "^0.12.6"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.7.0",
    "buffer": "^6.0.3",
    "destyle.css": "^3.0.2",
    "events": "^3.3.0",
    "music-metadata": "^7.12.3",
    "process": "^0.11.10",
    "solid-js": "^1.4.1"
  }
}
