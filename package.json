{
  "name": "drive-player",
  "version": "0.7.0",
  "description": "music files in google drive play on browser",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "start": "vite",
    "check": "run-s format lint type test",
    "format": "prettier -w index.html src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "watch": "run-p type:watch start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/drive-player.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/drive-player/issues"
  },
  "homepage": "https://github.com/mikiymk/drive-player#readme",
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.1",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vanilla-extract/vite-plugin": "^3.3.0",
    "eslint": "^8.21.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-solid": "~0.7.1",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-solid": "^2.3.0",
    "vitest": "^0.21.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.7.2",
    "buffer": "^6.0.3",
    "destyle.css": "^3.0.2",
    "events": "^3.3.0",
    "music-metadata": "^8.0.1",
    "process": "^0.11.10",
    "solid-js": "^1.4.8"
  },
  "overrides": {
    "@vanilla-extract/vite-plugin": {
      "vite": "^3"
    }
  }
}