{
  "name": "drive-player",
  "version": "0.7.1",
  "description": "music files in google drive play on browser",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:update-snapshot": "vitest run --update",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "start": "vite",
    "check": "run-z format lint2 type lint test",
    "format": "biome format --write",
    "lint": "eslint src api --cache",
    "lint:fix": "eslint src api --cache --fix",
    "lint2": "biome check .",
    "lint2:fix": "biome check . --write",
    "type": "tsc",
    "type:watch": "tsc --watch",
    "watch": "run-z type:watch start -p",
    "vercel:dev": "vercel dev",
    "vercel:prev": "vercel",
    "vercel:prod": "vercel --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/drive-player.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/drive-player/issues"
  },
  "homepage": "https://github.com/mikiymk/drive-player#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vanilla-extract/vite-plugin": "^5.0.0",
    "@vercel/node": "^5.1.2",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.20.0",
    "eslint-config-biome": "^1.9.4",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-solid": "~0.14.5",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "run-z": "^2.1.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vercel": "^41.0.2",
    "vite": "^6.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-solid": "^2.11.1",
    "vitest": "^3.0.5",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.17.1",
    "buffer": "^6.0.3",
    "destyle.css": "^4.0.1",
    "events": "^3.3.0",
    "music-metadata": "^10.9.0",
    "process": "^0.11.10",
    "solid-js": "^1.9.4"
  }
}
