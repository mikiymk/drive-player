{
  "name": "drive-player",
  "version": "0.3.0",
  "description": "music files in google drive play on browser",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "vite build",
    "build:wasm": "wasm-pack build ./wasm-sql --target web",
    "start": "vite",
    "check": "run-s type lint format test",
    "format": "prettier -w index.html src",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type": "tsc --noEmit",
    "type:watch": "tsc --noEmit --watch",
    "wasm": "wasm-pack build ./wasm-sql --target web",
    "wasm:watch": "cargo watch -s 'wasm-pack build ./wasm-sql --target web' -w ./wasm-sql",
    "watch": "run-p type:watch start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikiymk/drive-player.git"
  },
  "author": "mikiymk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikiymk/drive-player/issues"
  },
  "homepage": "https://github.com/mikiymk/drive-player#readme",
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vanilla-extract/vite-plugin": "^3.1.4",
    "eslint": "^8.11.0",
    "eslint-plugin-solid": "~0.4.7",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vite": "^2.9.5",
    "vite-plugin-eslint": "^1.6.0",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.7.0",
    "music-metadata-browser": "^2.5.5",
    "solid-js": "^1.3.17"
  }
}
